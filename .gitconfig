[alias]
    lg = log --graph --pretty=tformat:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%an %ar)%Creset'
    git = !exec git
[url "gh:"]
    insteadOf = https://github.com/
[safe]
    directory = /opt/esp-idf/components/openthread/openthread
    directory = /pfcalcul
