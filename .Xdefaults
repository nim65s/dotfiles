URxvt*depth:                32
URxvt*foreground:	        #DCDCCC
Urxvt*cursorColor:          white
URxvt*transparent:	        true
URxvt*fading:		        15
URxvt*tintColor:	        #5F5F5F
URxvt*blurRadius:           3
URxvt*iconFile:		        /home/nim/images/favicon.png
URxvt.font:		            xft:DejaVuSansMono-Oblique:pixelsize=9,xft:Code2000:antialias=false
URxvt*urgentOnBell: 	    True
URxvt*visualBell:           True
URxvt*loginShell:	        True
URxvt*scrollBar:	        False
URxvt*scrollTtyOutput:      True
URxvt*scrollTtyKeypress:    True
!URxvt*borderLess:           True
URxvt*cursorUnderline:      true
URxvt*termName:		        xterm-color

URxvt*perl-ext-common:	    default,fullscreen,matcher,url-select

URxvt*keysym.M-u:           perl:url-select:select_next
URxvt*underlineURLs:        true

URxvt*keysym.F11:           perl:fullscreen:switch
URxvt*urlLauncher:          chromium
URvxt*urlButton:            7

dXft*dpi:                    96
Xft*antialias:              True
Xft*hinting:                True
Xft*hintstyel:              full

xpdf*urlCommand:            /usr/bin/chromium %s

Xcursor*theme:              gruppled

xterm*faceName:             calibri
xterm*faceSize:             10

!URxvt*matcher.button:       1
!URxvt*matcher.pattern.1:    (?:www\\.)?youtube\\.com/watch\\?v=([^&]*)
!URxvt*matcher.launcher.1:   videoob -b youtube play $1@youtube
!URxvt*matcher.pattern.2:    (?:www\\.)?dailymotion.com/video/(.*)
!URxvt*matcher.launcher.2:   videoob play -b dailymotion $1@dailymotion
!URxvt*matcher.pattern.3:    (?:www\\.)?canalplus.fr/.*vid=([^&]*)
!URxvt*matcher.launcher.3:   videoob -b canalplus play $1@canalplus
