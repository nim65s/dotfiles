URxvt*depth:                32
URxvt*foreground:           #DCDCCC
Urxvt*cursorColor:          white
URxvt*transparent:          true
URxvt*fading:               15
URxvt*tintColor:            #5F5F5F
URxvt*blurRadius:           3
URxvt.font:                 xft:DejaVuSansMono-Oblique:pixelsize=9,xft:Code2000:antialias=false
URxvt*visualBell:           True
URxvt*loginShell:           True
URxvt*scrollBar:            False
URxvt*scrollTtyOutput:      True
URxvt*scrollTtyKeypress:    True
!URxvt*borderLess:           True
URxvt*cursorUnderline:      true
URxvt*termName:             xterm-color

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select

URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.url-select.underline: true

URxvt*keysym.F11:           perl:fullscreen:switch
URxvt.url-select.launcher:  chromium
URvxt*urlButton:            7

dXft*dpi:                    96
Xft*antialias:              True
Xft*hinting:                True
Xft*hintstyel:              full

xpdf*urlCommand:            /usr/bin/chromium %s

Xcursor*theme:              gruppled_white_lite

xterm*faceName:             calibri
xterm*faceSize:             10
